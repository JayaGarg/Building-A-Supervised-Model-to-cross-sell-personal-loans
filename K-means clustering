        "## Pick up the centroids and get the mean value of all three clusters."
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "DXzf5ZhMkRa4"
      },
      "source": [
        "centroids = {}\n",
        "for i in range(3):\n",
        "  result_list = []\n",
        "  result_list.append(df.loc[df['cluster'] == i]['x'].mean())\n",
        "  result_list.append(df.loc[df['cluster'] == i]['y'].mean())\n",
        "\n",
        "  centroids[i] = result_list"
      ],
      "execution_count": null,
